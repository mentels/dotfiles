[include]
        path = ~/.gituser
        # example contents of ~/.gituser
        # [user]
        # name = "Alice Ecila"
        # email = "alice.eclie@domain.com"
[core]
        editor = emacs
[alias]
        co = checkout
        ci = commit
        st = status
        br = branch
        hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
        type = cat-file -t
        dump = cat-file -p
